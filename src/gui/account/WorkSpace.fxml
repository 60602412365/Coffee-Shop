<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.account.WorkSpaceController">
   <children>
      <VBox prefHeight="600.0" prefWidth="159.0">
         <children>
            <Button fx:id="btn_Menu" mnemonicParsing="false" onAction="#_Menu" prefHeight="300.0" prefWidth="159.0" stylesheets="@WorkspaceColorPicker.css" text="Menu" textFill="WHITE">
               <graphic>
                  <ImageView fitHeight="35.0" fitWidth="35.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../Images/top_menu_24px.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="btn_Settings" mnemonicParsing="false" onAction="#_Settings" prefHeight="300.0" prefWidth="159.0" stylesheets="@WorkspaceColorPicker.css" text="Settings">
               <graphic>
                  <ImageView fitHeight="35.0" fitWidth="35.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../Images/settings_32px.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </children>
      </VBox>
      <VBox layoutX="159.0" prefHeight="600.0" prefWidth="274.0">
         <children>
            <ButtonBar prefHeight="53.0" prefWidth="203.0">
              <buttons>
                  <JFXTextField fx:id="jtf_search" prefHeight="25.0" promptText="Search" />
                <Button fx:id="btn_timKiem" mnemonicParsing="false" onAction="#_timKiem" prefHeight="31.0" text="Tìm kiếm" />
              </buttons>
            </ButtonBar>
         </children>
      </VBox>
   </children>
</AnchorPane>
